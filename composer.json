{
    "name": "os2web/os2web_hjemmeside8",
    "description": "Drupal 8 project for OS2Web Hjemmeside",
    "type": "project",
    "license": "EUPL-1.2",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bellcom/fds_base_theme"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/harvesthq/chosen-package.git",
                    "type": "git",
                    "reference": "v1.8.7"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bellcom/anonymous_subscriptions",
                "version": "8.x-1.x-dev",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/bellcom/anonymous_subscriptions.git",
                    "type": "git",
                    "reference": "8.x-1.x"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "bellcom/anonymous_subscriptions": "8.x-1.x-dev",
        "bellcom/fds_base_theme": "dev-stable/drupal-8.x",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/coder": "^8.3",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drupal/stage_file_proxy": "^1.0",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "mglaman/drupal-check": "^1.1",
        "os2web/os2web": "develop-dev",
        "os2web/os2web_borgerdk": "8.x-dev",
        "os2web/os2web_hearings": "dev-master",
        "os2web/os2web_logging": "dev-master",
        "os2web/os2web_meetings": "dev-develop",
        "os2web/os2web_meetings_sbsys": "dev-master",
        "os2web/os2web_pagebuilder": "dev-master",
        "os2web/os2web_search": "dev-master",
        "os2web/os2web_spotbox": "dev-master",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "patches": {
            "drupal/textfield_counter" : {
                "Fix scrolltop prevent" :  "./patches/textfield_counter-scrolltop-fix.patch"
            },
            "drupal/field_group": {
                "HTML5 Validation Prevents Submission in Tabs" : "https://www.drupal.org/files/issues/2019-10-04/field_group-HTML5_validation_prevents_submission_in_tabs-2969051-9.patch"
            }
        }
    }
}
